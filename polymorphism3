public class AA{
    int age;
    String name;
    double cgpa;
    int marks;
    AA(int age){
        this.age=age;
    }
    AA(int age,String name){
        this(age);
        this.name=name;
    }
    AA(int age,String name,double cgpa){
        this(age,name);
        this.cgpa=cgpa;
    }
    AA(int age,String name,double cgpa,int marks){
        this(age,name,cgpa);
        this.marks=marks;
    }
    public void display(){
        System.out.println(age);
        System.out.println(name);
        System.out.println(cgpa);
        System.out.println(marks);
    }
    public static void main(String[]args){
    new AA(10).display();
    new AA(10,"Siva").display();
    new AA(10,"Siva",8.0).display();
    new AA(10,"Siva",8.0,80).display();
    }
    }
